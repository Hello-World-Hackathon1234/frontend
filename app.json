{
  "expo": {
    "name": "OptiMeal",
    "slug": "OptiMeal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./Logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./Logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./Logo.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
